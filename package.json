{
  "name": "vue-simple-props",
  "version": "0.5.3",
  "packageManager": "pnpm@9.15.4",
  "description": "Remove emits, slots, and attrs concepts in Vue.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/vue-simple-props#readme",
  "bugs": {
    "url": "https://github.com/sxzz/vue-simple-props/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/vue-simple-props.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^4.6.0",
    "@sxzz/prettier-config": "^2.1.0",
    "@types/node": "^22.10.10",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.11.1",
    "eslint": "^9.19.0",
    "fast-glob": "^3.3.3",
    "happy-dom": "^16.7.3",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4",
    "vue": "^3.5.13"
  },
  "prettier": "@sxzz/prettier-config"
}
