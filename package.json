{
  "name": "vue-simple-props",
  "version": "0.6.3",
  "packageManager": "pnpm@10.22.0",
  "description": "Remove emits, slots, and attrs concepts in Vue.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/vue-simple-props#readme",
  "bugs": {
    "url": "https://github.com/sxzz/vue-simple-props/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/vue-simple-props.git"
  },
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.2.10",
    "@sxzz/prettier-config": "^2.2.4",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.10",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9",
    "vue": "^3.5.24"
  },
  "prettier": "@sxzz/prettier-config"
}
