{
  "name": "vue-simple-props",
  "version": "0.3.1",
  "packageManager": "pnpm@8.15.8",
  "description": "Remove emits, slots, and attrs concepts in Vue.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/vue-simple-props#readme",
  "bugs": {
    "url": "https://github.com/sxzz/vue-simple-props/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/vue-simple-props.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@sxzz/eslint-config": "^3.11.0",
    "@sxzz/prettier-config": "^2.0.2",
    "@types/node": "^20.12.12",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.4.1",
    "eslint": "^9.3.0",
    "fast-glob": "^3.3.2",
    "happy-dom": "^14.11.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "vue": "^3.4.27"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
